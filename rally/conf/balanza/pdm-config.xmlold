	<connector name="sftpConnector"
		className="com.pdm.interfaces.transports.RallySftpConnector">
		<properties>
    		<property name="pollingFrequency" value="30000"/>
    		<property name="autoDelete" value="false"/>
		</properties>
	</connector>
    <connector name="vmQueue" className="org.mule.providers.vm.VMConnector">
        <properties>
            <property name="queueEvents" value="true"/>
        </properties>
    </connector>
    <transformers>
        <transformer name="stringToEmailMessage" className="org.mule.providers.email.transformers.StringToEmailMessage"/>
        <transformer name="listOfMapsToMsgText" className="com.pdm.interfaces.transformers.ListOfMapsToMsgTextTransformer"/>
        <transformer name="messageEnvelopeExtractorTransformer"
			className="com.pdm.interfaces.transformers.MessageEnvelopeExtractorTransformer">
			<properties>
				<container-property reference="weightScaleMogRallyService"
					name="rallyService" />
			</properties>
		</transformer>
		<transformer name="listOfMapsToCSVTransformer"
			className="com.pdm.interfaces.transformers.ListOfMapsToDelimiterBasedTextFileTransformer">
			<properties>
				<property name="delimiter" value="DOTANDCOMMA" />
			</properties>
		</transformer>
		<transformer name="listOfMapsToTabSeparatedValuesTransformer"
			className="com.pdm.interfaces.transformers.ListOfMapsToDelimiterBasedTextFileTransformer">
			<properties>
				<property name="delimiter" value="TAB" />
			</properties>
		</transformer>
		<transformer name="listOfMapsToXLSTransformer"
			className="com.pdm.interfaces.transformers.ListOfMapsToXLSTransformer" />
		
		<transformer
			name="listOfMapsToFixedWidthBasedTextFileTransformerByColumnNames"
			className="com.pdm.interfaces.transformers.ListOfMapsToFixedWidthBasedTextFileTransformerByColumnNames">
			<properties>
				<map name="columnWidths">
				    <!-- Balanza de Mogi -->
				    <property name="SDI0080_WEIGHT_SCALE_MOG1" value="10;10;6;10;10;16;3;10;20;20;35" />
				    <property name="SDI0080_WEIGHT_SCALE_MOG2" value="10;10;6;10;10;16;3;10;20;20;35" />
					<property name="SDI0080_WEIGHT_SCALE_MOG3" value="10;10;6;10;10;16;3;10;20;20;35" />
                    <!-- Balanza de Mogi -->
					<!-- Balanza de Cabo -->
					<property name="SDI0080_WEIGHT_SCALE_CAB1" value="10;10;6;10;10;16;3;10;20;20;35" />
				    <property name="SDI0080_WEIGHT_SCALE_CAB2" value="10;10;6;10;10;16;3;10;20;20;35" />
                    <!-- Balanza de Cabo -->
                    <!-- Balanza de Balsa -->
					<property name="SDI0080_WEIGHT_SCALE_BAL1" value="10;10;6;10;10;16;3;10;20;20;35" />
					<!-- Balanza de Balsa -->
                    <!-- Balanza de Trombudo -->
					<property name="SDI0080_WEIGHT_SCALE_TRO1" value="10;10;6;10;10;16;3;10;20;20;35" />
					<!-- Balanza de Trombudo -->
                    <!-- Balanza de Conchal -->
					<property name="SDI0080_WEIGHT_SCALE_CON1" value="10;10;6;10;10;16;3;10;20;20;35" />
					<!-- Balanza de Conchal -->
				</map>
				<map name="columnNames">
					<!-- Balanza de Mogi -->
				    <property name="SDI0080_WEIGHT_SCALE_MOG1" value="numeroViaje;numeroPedido;numeroItem;codigoCliente;codigoLote;cantidad;unidadMedida;codigoTransportista;docChofer;nombreChofer;PATENTES" />
				    <property name="SDI0080_WEIGHT_SCALE_MOG2" value="numeroViaje;numeroPedido;numeroItem;codigoCliente;codigoLote;cantidad;unidadMedida;codigoTransportista;docChofer;nombreChofer;PATENTES" />
				    <property name="SDI0080_WEIGHT_SCALE_MOG3" value="numeroViaje;numeroPedido;numeroItem;codigoCliente;codigoLote;cantidad;unidadMedida;codigoTransportista;docChofer;nombreChofer;PATENTES" />
					<!-- Balanza de Mogi -->
					<!-- Balanza de Cabo -->
				    <property name="SDI0080_WEIGHT_SCALE_CAB1" value="numeroViaje;numeroPedido;numeroItem;codigoCliente;codigoLote;cantidad;unidadMedida;codigoTransportista;docChofer;nombreChofer;PATENTES" />
				    <property name="SDI0080_WEIGHT_SCALE_CAB2" value="numeroViaje;numeroPedido;numeroItem;codigoCliente;codigoLote;cantidad;unidadMedida;codigoTransportista;docChofer;nombreChofer;PATENTES" />
					<!-- Balanza de Cabo -->
					<!-- Balanza de Balsa -->
				    <property name="SDI0080_WEIGHT_SCALE_BAL1" value="numeroViaje;numeroPedido;numeroItem;codigoCliente;codigoLote;cantidad;unidadMedida;codigoTransportista;docChofer;nombreChofer;PATENTES" />
					<!-- Balanza de Balsa -->
					<!-- Balanza de Trombudo -->
				    <property name="SDI0080_WEIGHT_SCALE_TRO1" value="numeroViaje;numeroPedido;numeroItem;codigoCliente;codigoLote;cantidad;unidadMedida;codigoTransportista;docChofer;nombreChofer;PATENTES" />
					<!-- Balanza de Trombudo -->
					<!-- Balanza de Conchal -->
				    <property name="SDI0080_WEIGHT_SCALE_CON1" value="numeroViaje;numeroPedido;numeroItem;codigoCliente;codigoLote;cantidad;unidadMedida;codigoTransportista;docChofer;nombreChofer;PATENTES" />
					<!-- Balanza de Conchal -->
				</map>
			</properties>
		</transformer>
		<transformer
			name="fixedWidthBasedTextFileToListOfMapsTransformer"
			className="com.pdm.interfaces.transformers.FixedWidthBasedTextFileToListOfMapsTransformer">
			<properties>
				<map name="columnWidths">
		                    <property name="SDI0070_WEIGHT_SCALE_MOG"
						value="3;4;4;16;10;8;10;6;10;40;18;40;10;17;3;3" />
		                    <property name="SDI0070_WEIGHT_SCALE_CAB"
						value="3;4;4;16;10;8;10;6;10;40;18;40;10;17;3;3" />
		                    <property name="SDI0070_WEIGHT_SCALE_BAL"
