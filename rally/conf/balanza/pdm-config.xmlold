<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mule-configuration PUBLIC "-//MuleSource //DTD mule-configuration XML V1.0//EN"
                                "http://mule.mulesource.org/dtds/mule-configuration.dtd">
<mule-configuration id="PDM-Balanza" version="1.0">
	<description>Rally</description>
	<environment-properties>
		<file-properties location="../rally.properties"/>
	</environment-properties>
	<mule-environment-properties serverUrl="" enableMessageEvents="true" synchronous="true"/>
	<container-context
		className="org.mule.extras.spring.SpringContainerContext">
		<properties>
			<property name="configFile" value="application-context.xml" />
		</properties>
	</container-context>
	<agents>
		<agent name="rmiAgent"
			className="org.mule.management.agents.RmiRegistryAgent">
			<properties>
				<property name="serverUri" value="rmi://localhost:1398" />
			</properties>
		</agent>
        <agent name="jmxAgent" className="com.pdm.interfaces.agents.PDMJmxAgent">
			<properties>
				<property name="connectorServerUrl"
					value="service:jmx:rmi:///jndi/rmi://localhost:1398/server" />
				<map name="connectorServerProperties">
					<property name="jmx.remote.jndi.rebind"
						value="true" />
				</map>
<!--
				<map name="credentials">
					<property name="muleusername" value="clizarralde" />
					<property name="mulepassword" value="oycobe" />
				</map>
-->
			</properties>
		</agent>
		<agent name="mx4jAgent"
			className="org.mule.management.agents.Mx4jAgent">
			<properties>
				<property name="jmxAdaptorUrl"
					value="http://0.0.0.0:9998" />
				<property name="login" value="clizarralde" />
				<property name="password" value="oycobe" />
			</properties>
		</agent>
		<agent name="controlAndLogAgent"
			className="com.pdm.interfaces.MessageEnvelopeBasedComponentControlAndLogAgent">
			<properties>
				<container-property reference="weightScaleMogRallyService"
					name="rallyService" />
			</properties>
		</agent>
	</agents>
	<connector name="controlConnector"
		className="org.mule.providers.jdbc.JdbcConnector">
		<properties>
			<map name="queries">
				<property name="MMI0010_WEIGHT_SCALE_OUT"
					value="insert into ZCPIMMI0010 (ID_INTERFACE, MATERIAL, PLANT, STORAGE_LOC, PURCHASE_ORDER, DELIVERY, BATCH, MOV_TYPE, QUANTITY, UNIT, PRODUCTION_DT, EXPIRATION_DT, DOCUMENT_DT, COST_CENTER, REC_STORAGE_LOC, QTY_DEL, BASE_UOM, REC_PLANT, REC_BATCH, VENDOR, BILL_LADING, CONTAINERS, UNIT_CONTAINER, VEHICLE_PLATE, GROSS_WEIGHT, UNIT_GROSS, VEHICLE_WEIGHT, UNIT_VEHICLE, CODIGO_IVA, BASE_IMPUESTO, NRO_ALEATORIO, DIGITO_VERIF, ISSUE_TYPE, PORC_HUME, PROTOCOLO, DOCNF_DT) values (${INTERFACE_ID}, ${MATERIAL}, ${PLANT}, ${STORAGE_LOC}, ${PURCHASE_ORDER}, ${DELIVERY}, ${BATCH}, ${MOV_TYPE}, ${QUANTITY}, ${UNIT}, ${PRODUCTION_DT}, ${EXPIRATION_DT}, ${DOCUMENT_DT}, ${COST_CENTER}, ${REC_STORAGE_LOC}, ${QTY_DEL}, ${BASE_UOM}, NULL, NULL, NULL, ${BILL_OF_LADING}, NULL, NULL, NULL, NULL, NULL, NULL, NULL, ${CODIGOIVA}, ${BASEDEIMPUESTO}, ${NUMEROALEATORIO}, ${DIGITOVERIFICADOR}, ${ISSUETYPE}, ${TYPE}, ${PROTOCOLO}, ${DOCNF_DT})" />
            </map>
			<container-property name="dataSource"
				reference="dataSourceControl" />
		</properties>
		<exception-strategy
			className="com.pdm.interfaces.MessageEnvelopeBasedComponentAndConnectorExceptionListener">
			<properties>
				<container-property reference="weightScaleMogRallyService"
					name="rallyService" />
			</properties>
		</exception-strategy>
	</connector>
    <connector name="weightScaleMogConnector"
		className="org.mule.providers.jdbc.JdbcConnector">
		<properties>
			<map name="queries">
				<property name="MMI0010_WEIGHT_SCALE_MOG_TRIGGER"
					value="select STORAGE_LOC, DELIVERY from sap_recepcion where PROCESSED='N' and ORGANIZATION_CODE='4120' LIMIT 1" />
				<property name="SDI0080_WEIGHT_SCALE_MOG1_TRIGGER"
					value="select numeroViaje from sap_entrega where ACK!='S' and organizacion='4120' and puesto='MOG1' LIMIT 1" />
				<property name="SDI0080_WEIGHT_SCALE_MOG2_TRIGGER"
					value="select numeroViaje from sap_entrega where ACK!='S' and organizacion='4120' and puesto='MOG2' LIMIT 1" />
				<property name="SDI0080_WEIGHT_SCALE_MOG3_TRIGGER"
					value="select numeroViaje from sap_entrega where ACK!='S' and organizacion='4120' and puesto='MOG3' LIMIT 1" />
				<property name="MMI0040_WEIGHT_SCALE_OUT"
					value="insert into sap_producto (ACK,MATNR,WERKS,MAKTX,MEINS,BRGEW,GEWEI,NTGEW,tipoMaterial) values ('N',${MATERIAL_NUMB},${PLANT},${DESC_MAT_PT},${UNIT_MEASURE},${GROSS_WEIGHT},${WEIGHT_UNIT},${NEW_WEIGHT},${MATERIAL_TYPE})" />
				<property name="MMI0100_WEIGHT_SCALE_OUT"
					value="insert into sap_orden_de_compra (EBELP, LGORT, MEINS, BPRME, EKORG, EBELN, TXZ01, MATNR, BUKRS, MENGE, LIFNR, WERKS) values (${EBELP}, ${LGORT}, ${MEINS}, ${BPRME}, ${EKORG}, ${EBELN}, ${TXZ01}, ${MATNR}, ${BUKRS}, ${MENGE}, ${LIFNR}, ${WERKS}) " />
				<property name="MMI0090_WEIGHT_SCALE_OUT"
					value="insert into sap_proveedor (ACK, LIFNR, BUKRS, NAME1, STCD1, EKORG, REGIO) values ('N', ${LIFNR}, ${BUKRS}, ${NAME1}, ${STCD1}, ${EKORG}, substr(${REGIO},1,2)) " />
				<property name="SDI0070_WEIGHT_SCALE_OUT"
					value="insert into sap_shipment (pais, planta, almacen, numeroViaje, fechaViaje, numeroPedido, numeroLinea, codigoCliente, cliente, codProducto, descProducto, lote, cantidad, unidadMedida, incoterms) values (${PAIS}, ${PLANTA}, ${ALMACEN}, ${NUMERO_VIAJE}, ${FECHA_VIAJE}, ${NUMERO_PEDIDO}, ${NUMERO_LINEA}, ${CODIGO_CLIENTE}, ${CLIENTE}, ${CODIGO_PRODUCTO}, ${DESC_PRODUCTO}, ${LOTE}, ${CANTIDAD}, ${UNIDAD_MEDIDA}, ${INCOTERMS}) " />
			</map>
			<container-property name="dataSource"
				reference="dataSourceWeightScaleMog" />
		</properties>
		<exception-strategy
			className="com.pdm.interfaces.MessageEnvelopeBasedComponentAndConnectorExceptionListener">
			<properties>
				<container-property reference="weightScaleMogRallyService"
					name="rallyService" />
			</properties>
		</exception-strategy>
	</connector>
